void screens()
{
  switch (a) {

  case 0:
    screen1();

    break;

  case 1:
    screen2();
    break;


  case 2:
    screen3();
    break;


  case 3: 
    screen4();
    translate(24, 42);
    scale(.4);
    loop.LOOP();
    translate(382, 132);
    scale(.7);
    milo.MILO();
    translate(400, -140);
    scale(1.3);
    fungi.FUNGI();
    translate(-663, 580);
    scale(.9);
    dutter.DUTTER();
    translate(270, -95);
    scale(1.3);
    piber.PIBER();
    translate(327, 30);
    scale(.9); 
    tini.TINI();
    textSize(40);
    text("Loop", -395, -75);
    text("Milo", -68, -73);
    text("Fungi", 250, -70);
    text("Dutter", -380, 450);
    text("Piber", -57, 460);
    text("Tini", 260, 460);
    text("1", -500, -450);
    text("2", -170, -450);
    text("3", 170, -450);
    text("4", -500, 70);
    text("5", -170, 70);
    text("6", 160, 70);
    break;






  case 4:
    screen5();
    break;


  case 5:
    screen6();
    break;

  case 6:
    screen7();
    break;

    //como ponerlas?
  case 7:
    screen10();
    break;
  }
}




void screen1() {
  background(#4ECE2C);
  fill(255);
  textAlign(CENTER, BOTTOM);
  textSize(40);
  text("ALIENWAR", 250, 100);
  textSize(20);
  textAlign(CENTER, TOP);
  text("Presiona cualquier tecla para comenzar", 250, 400);
  nave.NAVE();
}

void screen2()
{
