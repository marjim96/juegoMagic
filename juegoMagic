  fill(255, 0, 0);
  textSize(16);
  text("*Elige a tu personaje y combate por llegar primero a conquistar la tierra*", 100, 100, 300, 70);
  fill(255);
  textSize(13);
  text("Para seguir avanzando continúa presionando cualquier tecla", 250, 400);
  tierra.TIERRA();
}

void screen3() {
  fill(255);
  String c= "Para jugar necesitas a un contrincante, cada quien debe elegir a un personaje y atacar por turnos para poder ganar";
  background(#2345E8);
  textAlign(CENTER, CENTER);
  textSize(30);
  text(c, 50, 50, 400, 400);
}

void screen4() 
{
  background(0);
  noStroke();
  fill(#111F36);
  rectMode(CENTER);
  rect(width*.25, 150, 100, 170);
  rect(width*.50, 150, 100, 170);
  rect(width*.75, 150, 100, 170);
  rect(width*.25, 350, 100, 170);
  rect(width*.50, 350, 100, 170);
  rect(width*.75, 350, 100, 170);
  election();
}

void election()
{
  if (J1 == 0)
  {
    switch (key) 
    {

    case '1':
      J1 = 1;
      key = '9'; 
      break;

    case '2':

      J1 = 2;
      key = '9';
      break;

    case '3':

      J1 = 3;
      key = '9';
      break;

    case '4':
      J1 = 4;
      key = '9';
      break;

    case '5':
      J1 = 5;
      key = '9';
      break;

    case '6':
      J1 = 6;
      key = '9';
      break;
    }  


    textSize(15);
    fill(255);
    text("Jugador 1 elige presionando el numero", 250, 460);
  }



  if (J1 != 0)

  {
    textSize(15);
    fill(255);
    text("Jugador 2 elige presionando el numero", 250, 460);
    switch (key) 
    {
    case '1':
      J2 = 1;
      key = '9';
      break;

    case '2':

      J2 = 2;
      key = '9';
      break;

    case '3':

      J2 = 3;
      key = '9';
      break;

    case '4':
      J2 = 4;
      key = '9';
      break;

    case '5':
      J2 = 5;
      key = '9';
      break;

    case '6':
      J2 = 6;
      key = '9';
      break;
      
    }
  }
}


